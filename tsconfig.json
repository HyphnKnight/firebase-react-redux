{
  "compilerOptions": {
    "diagnostics": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "rootDir": "src",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ESNext"
  }
}
